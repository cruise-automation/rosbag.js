{
  "name": "rosbag",
  "version": "3.0.1",
  "license": "Apache-2.0",
  "repository": "cruise-automation/rosbag.js",
  "dependencies": {
    "buffer": "5.2.1",
    "heap": "0.2.7",
    "int53": "1.0.0"
  },
  "engines": {
    "node": ">=12.7.0"
  },
  "exports": {
    "browser": "./dist/web/index.js",
    "require": "./dist/node/index.js",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepack": "yarn run build",
    "lint": "yarn run tsc --noEmit && eslint src && prettier -l 'src/**/*.ts'",
    "lint:fix": "eslint --fix src && prettier --write -l 'src/**/*.ts'",
    "test": "yarn run jest --verbose && yarn run lint",
    "clean": "rm -rf build dist",
    "build": "yarn run clean && mkdir -p dist && yarn run build-node && yarn run build-web",
    "build-node": "cross-env ROSBAG_TARGET=node webpack --mode development",
    "build-web": "cross-env ROSBAG_TARGET=web webpack --mode development"
  },
  "devDependencies": {
    "@types/heap": "0.2.31",
    "@types/int53": "1.0.0",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "compressjs": "1.0.3",
    "cross-env": "5.2.0",
    "eslint": "8.37.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "jest": "29.5.0",
    "jest-cli": "29.5.0",
    "jest-junit": "5.2.0",
    "lz4js": "0.2.0",
    "prettier": "2.8.7",
    "regenerator-runtime": "0.12.1",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "typescript": "5.0.2",
    "webpack": "5.77.0",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "preset": "ts-jest"
  }
}
