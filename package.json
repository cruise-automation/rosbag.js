{
  "name": "rosbag",
  "version": "2.1.0",
  "license": "Apache-2.0",
  "repository": "cruise-automation/rosbag.js",
  "dependencies": {
    "buffer": "5.2.1",
    "heap": "0.2.6",
    "int53": "0.2.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "dist/node",
  "browser": "dist/web",
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepack": "yarn run build",
    "lint": "yarn run flow && eslint src && prettier -l 'src/**/*.js'",
    "test": "yarn run jest --verbose && yarn run lint",
    "clean": "rm -rf build dist",
    "build": "yarn run clean && mkdir -p dist/ && yarn run flow && yarn run build-node && yarn run build-web && yarn run flow-copy-source src dist",
    "build-node": "ROSBAG_TARGET=node webpack --mode development",
    "build-web": "ROSBAG_TARGET=web webpack --mode development"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-flow": "7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "compressjs": "1.0.3",
    "eslint": "5.7.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-flow-header": "0.2.0",
    "eslint-plugin-flowtype": "3.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "flow-bin": "0.83.0",
    "flow-copy-source": "2.0.2",
    "flow-typed": "2.5.1",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-junit": "5.2.0",
    "lz4js": "0.2.0",
    "prettier": "1.14.3",
    "regenerator-runtime": "0.12.1",
    "webpack": "4.21.0",
    "webpack-cli": "3.1.2",
    "webpack-node-externals": "1.7.2"
  },
  "prettier": {
    "arrowParens": "always",
    "jsxBracketSameLine": true,
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ]
  }
}
